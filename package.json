{
  "name": "apicontest_team_appcomposer",
  "version": "1.0.0",
  "description": "",
  "main": "script.js",
  "scripts": {
    "start": "nodemon -V --exec ts-node src/main.ts  --ignore gcp_asserts",
    "diststart": "node -r ./src/main.ts",
    "newstart:watch": "nodemon",
    "build:local": "tsc -p ./src",
    "build:prod": "npm run clean:dest && webpack --mode=development",
    "clean:dest": "rimraf dist",
    "test": "mocha **/*.test.ts --reportor mochawesome",
    "cover": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "author": "TeamAppComposer",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/body-parser": "1.17.0",
    "@types/chai": "^4.2.7",
    "@types/chai-http": "^4.2.0",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "body-parser": "1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "child_process": "^1.0.2",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^5.1.1",
    "cors": "^2.8.5",
    "del": "^5.1.0",
    "express": "^4.17.1",
    "https": "^1.0.0",
    "ip": "^1.1.5",
    "joi": "^13.1.0",
    "lodash": "^4.17.15",
    "mocha": "^7.0.0",
    "mongodb": "^3.5.2",
    "multimatch": "^4.0.0",
    "mysql": "^2.18.1",
    "rimraf": "^3.0.1",
    "shelljs": "^0.8.3",
    "swagger-ui-express": "^4.1.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.20.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-node-externals": "1.7.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.8",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "mochawesome": "^5.0.0",
    "nodemon": "^1.19.2",
    "nyc": "^15.0.0",
    "request": "^2.88.0",
    "ts-node": "^8.4.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "gcp_assets"
    ],
    "watch": [
      ".src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "nyc": {
    "include": [
      "test/**/*.test.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "**/*.spec.ts",
      "**/*.e2e-spec.ts",
      "**/*.po.ts",
      "gcp_assets"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "mocha": {
    "diff": true,
    "extension": [
      "ts"
    ],
    "package": "./package.json",
    "reporter": "mochawesome",
    "slow": 75,
    "timeout": 2000,
    "ui": "bdd",
    "watch-files": [
      "test/**/*.ts"
    ],
    "watch-ignore": [
      "lib/vendor"
    ]
  }
}
